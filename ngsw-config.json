{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "burger",
      "urls": [
        "https://burguer-queen-e0998.firebaseio.com/**"
      ],
      "cacheConfig": {
        "startegy": "freshness",
        "maxAge": "2h",
        "maxSize": 100,
        "timeout": "8s"
      }
    }
  ]
}
